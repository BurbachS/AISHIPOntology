<?xml version="1.0"?>
<rdf:RDF xmlns="https://burbachs.github.io/AISHIPOntology/AISHIP#"
     xml:base="https://burbachs.github.io/AISHIPOntology/AISHIP"
     xmlns:dc="http://purl.org/dc/elements/1.1/"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:xml="http://www.w3.org/XML/1998/namespace"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:bibo="http://purl.org/ontology/bibo/"
     xmlns:foaf="http://xmlns.com/foaf/0.1/"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:terms="http://purl.org/dc/terms/">
    <owl:Ontology rdf:about="https://burbachs.github.io/AISHIPOntology/AISHIP">
        <owl:imports rdf:resource="http://qudt.org/3.1.2/schema/qudt"/>
        <owl:imports rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#Ontology"/>
        <terms:bibliographicCitation>Simon Burbach, Lennart Mackert, Maria Maleshkova (2025): AISHIP Ontology. https://doi.org/10.5281/zenodo.16704863</terms:bibliographicCitation>
        <terms:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2025-07-23</terms:created>
        <terms:creator rdf:nodeID="genid1"/>
        <terms:creator rdf:nodeID="genid3"/>
        <terms:creator rdf:nodeID="genid5"/>
        <terms:license rdf:resource="https://creativecommons.org/licenses/by-nc-sa/4.0/"/>
        <terms:title>AISHIP Ontology</terms:title>
        <bibo:doi>10.5281/zenodo.16704863</bibo:doi>
        <rdfs:comment xml:lang="en">AISHIP is an ontology designed to enable semantic integration and enrichment of heterogeneous maritime data. It extends the VesselAI ontology with enhanced vessel characteristics, detailed propulsion systems, extended trajectory modeling, and multimodal vessel representations.</rdfs:comment>
    </owl:Ontology>
    <rdf:Description rdf:nodeID="genid1"/>
    <rdf:Description rdf:nodeID="genid3"/>
    <rdf:Description rdf:nodeID="genid5"/>
    


