    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Datatypes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->


    


    <!-- http://www.w3.org/2001/XMLSchema#date -->


    <rdfs:Datatype rdf:about="http://www.w3.org/2001/XMLSchema#date"/>
    


    <!-- http://www.w3.org/2001/XMLSchema#gYear -->


    <rdfs:Datatype rdf:about="http://www.w3.org/2001/XMLSchema#gYear"/>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->


    


    <!-- http://qudt.org/schema/qudt/unit -->


    <owl:ObjectProperty rdf:about="http://qudt.org/schema/qudt/unit"/>
    


    <!-- http://www.vesselAI-project.eu/ontology#hasGeometry -->


    <owl:ObjectProperty rdf:about="http://www.vesselAI-project.eu/ontology#hasGeometry"/>
    


    <!-- https://burbachs.github.io/AISHIPOntology/AISHIP#computedOverTimeInterval -->


    <owl:ObjectProperty rdf:about="https://burbachs.github.io/AISHIPOntology/AISHIP#computedOverTimeInterval">
        <rdfs:domain rdf:resource="https://burbachs.github.io/AISHIPOntology/AISHIP#TypicalLocation"/>
        <rdfs:range rdf:resource="http://www.w3.org/2006/time#Interval"/>
        <rdfs:comment xml:lang="en">Relatesa typical location with the time interval it was computed over.</rdfs:comment>
        <rdfs:label xml:lang="en">computed over time interval</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://burbachs.github.io/AISHIPOntology/AISHIP#drivenBy -->


    <owl:ObjectProperty rdf:about="https://burbachs.github.io/AISHIPOntology/AISHIP#drivenBy">
        <owl:inverseOf rdf:resource="https://burbachs.github.io/AISHIPOntology/AISHIP#drives"/>
        <rdfs:domain rdf:resource="https://burbachs.github.io/AISHIPOntology/AISHIP#MotorizedPropulsionSystem"/>
        <rdfs:range rdf:resource="https://schema.org/EngineSpecification"/>
        <rdfs:comment xml:lang="en">Relates a motorized propulsion technology with the engine it is driven by.</rdfs:comment>
        <rdfs:label xml:lang="en">driven by</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://burbachs.github.io/AISHIPOntology/AISHIP#drives -->


    <owl:ObjectProperty rdf:about="https://burbachs.github.io/AISHIPOntology/AISHIP#drives">
        <rdfs:domain rdf:resource="https://schema.org/EngineSpecification"/>
        <rdfs:range rdf:resource="https://burbachs.github.io/AISHIPOntology/AISHIP#MotorizedPropulsionTechnology"/>
        <rdfs:comment xml:lang="en">Relates an engine to the motorized propulsion technology it drives.</rdfs:comment>
        <rdfs:label xml:lang="en">drives</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://burbachs.github.io/AISHIPOntology/AISHIP#hasActualHeight -->


    <owl:ObjectProperty rdf:about="https://burbachs.github.io/AISHIPOntology/AISHIP#hasActualHeight">
        <rdfs:domain rdf:resource="http://www.vesselAI-project.eu/ontology#Vessel"/>
        <rdfs:range>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://qudt.org/schema/qudt/QuantityValue"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://qudt.org/schema/qudt/unit"/>
                        <owl:hasValue rdf:resource="https://qudt.org/3.1.2/vocab/unitM"/>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://qudt.org/schema/qudt/numericValue"/>
                        <owl:allValuesFrom rdf:resource="http://www.w3.org/2001/XMLSchema#decimal"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </rdfs:range>
        <rdfs:comment xml:lang="en">Relates a vessel to its actual height. Indicates the actual current height of the vessel, e.g., considering load or structural changes, expressed in meters. Note: This property is semantically the same as http://www.vesselAI-project.eu/ontology#hasActualHeight. However, we allow the expression as a quantity with numeric value and a unit.</rdfs:comment>
        <rdfs:label xml:lang="en">has actual height</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://burbachs.github.io/AISHIPOntology/AISHIP#hasAntiCavitationMechanism -->


    <owl:ObjectProperty rdf:about="https://burbachs.github.io/AISHIPOntology/AISHIP#hasAntiCavitationMechanism">
        <owl:inverseOf rdf:resource="https://burbachs.github.io/AISHIPOntology/AISHIP#isAntiCavitationMechanismFor"/>
        <rdfs:domain rdf:resource="https://burbachs.github.io/AISHIPOntology/AISHIP#MotorizedPropulsionTechnology"/>
        <rdfs:range rdf:resource="https://burbachs.github.io/AISHIPOntology/AISHIP#AntiCavitationMechanism"/>
        <rdfs:comment xml:lang="en">Relates a motorized propulsion technology to its anti-cavitation mechanism that reduces its cavitation.</rdfs:comment>
        <rdfs:label xml:lang="en">has anti-cavitation mechanism</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://burbachs.github.io/AISHIPOntology/AISHIP#hasAverageSpeedOverGround -->


    <owl:ObjectProperty rdf:about="https://burbachs.github.io/AISHIPOntology/AISHIP#hasAverageSpeedOverGround">
        <rdfs:domain rdf:resource="http://www.vesselAI-project.eu/ontology#Vessel"/>
        <rdfs:range>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://qudt.org/schema/qudt/QuantityValue"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://qudt.org/schema/qudt/unit"/>
                        <owl:hasValue rdf:resource="https://qudt.org/3.1.2/vocab/unitKN"/>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://qudt.org/schema/qudt/numericValue"/>
                        <owl:allValuesFrom rdf:resource="http://www.w3.org/2001/XMLSchema#decimal"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </rdfs:range>
        <rdfs:comment xml:lang="en">Relates a vessel with its overall average speed over ground, typically measured in knots (kn).</rdfs:comment>
        <rdfs:label xml:lang="en">has average speed over ground</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://burbachs.github.io/AISHIPOntology/AISHIP#hasAverageTrajectorySpeedOverGround -->


    <owl:ObjectProperty rdf:about="https://burbachs.github.io/AISHIPOntology/AISHIP#hasAverageTrajectorySpeedOverGround">
        <rdfs:domain rdf:resource="http://www.vesselAI-project.eu/ontology#Trajectory"/>
        <rdfs:range>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://qudt.org/schema/qudt/QuantityValue"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://qudt.org/schema/qudt/unit"/>
                        <owl:hasValue rdf:resource="https://qudt.org/3.1.2/vocab/unitKN"/>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://qudt.org/schema/qudt/numericValue"/>
                        <owl:allValuesFrom rdf:resource="http://www.w3.org/2001/XMLSchema#decimal"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </rdfs:range>
        <rdfs:comment xml:lang="en">Relates a trajtectory to the average speed over ground of the vessel during that trip, typically measured in knots (kn).</rdfs:comment>
        <rdfs:label xml:lang="en">has average trajectory speed over ground</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://burbachs.github.io/AISHIPOntology/AISHIP#hasCourseOverGround -->


    <owl:ObjectProperty rdf:about="https://burbachs.github.io/AISHIPOntology/AISHIP#hasCourseOverGround">
        <rdfs:domain rdf:resource="http://www.vesselAI-project.eu/ontology#Vessel"/>
        <rdfs:range>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://qudt.org/schema/qudt/QuantityValue"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://qudt.org/schema/qudt/unit"/>
                        <owl:hasValue rdf:resource="https://qudt.org/3.1.2/vocab/unitDEG"/>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://qudt.org/schema/qudt/numericValue"/>
                        <owl:allValuesFrom>
                            <rdfs:Datatype>
                                <owl:onDatatype rdf:resource="http://www.w3.org/2001/XMLSchema#decimal"/>
                                <owl:withRestrictions rdf:parseType="Collection">
                                    <rdf:Description>
                                        <xsd:minInclusive rdf:datatype="http://www.w3.org/2001/XMLSchema#decimal">0.0</xsd:minInclusive>
                                    </rdf:Description>
                                    <rdf:Description>
                                        <xsd:maxInclusive rdf:datatype="http://www.w3.org/2001/XMLSchema#decimal">360.0</xsd:maxInclusive>
                                    </rdf:Description>
                                </owl:withRestrictions>
                            </rdfs:Datatype>
                        </owl:allValuesFrom>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </rdfs:range>
        <rdfs:comment xml:lang="en">Relates a vessel to its course over ground. Indicates the actual course of a vessel over the ground, given as an angle relative to true north (in degrees). Note: This property is semantically the same as http://www.vesselAI-project.eu/ontology#hasCourseOverGround. However, we allow the expression as a quantity with numeric value and a unit.</rdfs:comment>
        <rdfs:label xml:lang="en">has course over ground</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://burbachs.github.io/AISHIPOntology/AISHIP#hasDeadweight -->


    <owl:ObjectProperty rdf:about="https://burbachs.github.io/AISHIPOntology/AISHIP#hasDeadweight">
        <rdfs:domain rdf:resource="http://www.vesselAI-project.eu/ontology#Vessel"/>
        <rdfs:range>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://qudt.org/schema/qudt/QuantityValue"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://qudt.org/schema/qudt/unit"/>
                        <owl:hasValue rdf:resource="https://qudt.org/3.1.2/vocab/unitTON_Metric"/>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://qudt.org/schema/qudt/numericValue"/>
                        <owl:allValuesFrom rdf:resource="http://www.w3.org/2001/XMLSchema#decimal"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </rdfs:range>
        <rdfs:comment xml:lang="en">Relates a vessel to its deadweight. Deadweight tonnage (DWT) represents the maximum weight of cargo, fuel, crew, provisions, and other supplies the vessel can carry, expressed in metric tons. Note: This property is semantically the same as http://www.vesselAI-project.eu/ontology#hasDeadWeight. However, we allow the expression as a quantity with numeric value and a unit.</rdfs:comment>
        <rdfs:label xml:lang="en">has deadweight</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://burbachs.github.io/AISHIPOntology/AISHIP#hasDescription -->


    <owl:ObjectProperty rdf:about="https://burbachs.github.io/AISHIPOntology/AISHIP#hasDescription">
        <owl:inverseOf rdf:resource="https://burbachs.github.io/AISHIPOntology/AISHIP#isDescriptionOf"/>
        <rdfs:domain rdf:resource="http://www.vesselAI-project.eu/ontology#Vessel"/>
        <rdfs:range rdf:resource="http://www.semanticdesktop.org/ontologies/2007/03/22/nfo#TextDocument"/>
        <rdfs:comment xml:lang="en">Relates a vessel to its text document description.</rdfs:comment>
        <rdfs:label xml:lang="en">has description</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://burbachs.github.io/AISHIPOntology/AISHIP#hasDiameter -->


    <owl:ObjectProperty rdf:about="https://burbachs.github.io/AISHIPOntology/AISHIP#hasDiameter">
        <rdfs:domain rdf:resource="https://burbachs.github.io/AISHIPOntology/AISHIP#BladedPropulsor"/>
        <rdfs:range>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://qudt.org/schema/qudt/QuantityValue"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://qudt.org/schema/qudt/unit"/>
                        <owl:hasValue rdf:resource="https://qudt.org/3.1.2/vocab/unitCentiM"/>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://qudt.org/schema/qudt/numericValue"/>
                        <owl:allValuesFrom rdf:resource="http://www.w3.org/2001/XMLSchema#decimal"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </rdfs:range>
        <rdfs:comment xml:lang="en">Relates a bladed propulsor to its diameter.</rdfs:comment>
        <rdfs:label xml:lang="en">has diameter</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://burbachs.github.io/AISHIPOntology/AISHIP#hasDimensionA -->


    <owl:ObjectProperty rdf:about="https://burbachs.github.io/AISHIPOntology/AISHIP#hasDimensionA">
        <rdfs:domain rdf:resource="http://www.vesselAI-project.eu/ontology#Vessel"/>
        <rdfs:range>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://qudt.org/schema/qudt/QuantityValue"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://qudt.org/schema/qudt/unit"/>
                        <owl:hasValue rdf:resource="https://qudt.org/3.1.2/vocab/unitM"/>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://qudt.org/schema/qudt/numericValue"/>
                        <owl:allValuesFrom rdf:resource="http://www.w3.org/2001/XMLSchema#decimal"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </rdfs:range>
        <rdfs:comment xml:lang="en">Relates a vessel to its dimension A, i. e. the distance from the GPS antenna to the bow.</rdfs:comment>
        <rdfs:label xml:lang="en">has dimension A</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://burbachs.github.io/AISHIPOntology/AISHIP#hasDimensionB -->


    <owl:ObjectProperty rdf:about="https://burbachs.github.io/AISHIPOntology/AISHIP#hasDimensionB">
        <rdfs:domain rdf:resource="http://www.vesselAI-project.eu/ontology#Vessel"/>
        <rdfs:range>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://qudt.org/schema/qudt/QuantityValue"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://qudt.org/schema/qudt/unit"/>
                        <owl:hasValue rdf:resource="https://qudt.org/3.1.2/vocab/unitM"/>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://qudt.org/schema/qudt/numericValue"/>
                        <owl:allValuesFrom rdf:resource="http://www.w3.org/2001/XMLSchema#decimal"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </rdfs:range>
        <rdfs:comment xml:lang="en">Relates a vessel to its dimension B, i. e. the distance from the GPS antenna to the stern.</rdfs:comment>
        <rdfs:label xml:lang="en">has dimension B</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://burbachs.github.io/AISHIPOntology/AISHIP#hasDimensionC -->


    <owl:ObjectProperty rdf:about="https://burbachs.github.io/AISHIPOntology/AISHIP#hasDimensionC">
        <rdfs:domain rdf:resource="http://www.vesselAI-project.eu/ontology#Vessel"/>
        <rdfs:range>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://qudt.org/schema/qudt/QuantityValue"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://qudt.org/schema/qudt/unit"/>
                        <owl:hasValue rdf:resource="https://qudt.org/3.1.2/vocab/unitM"/>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://qudt.org/schema/qudt/numericValue"/>
                        <owl:allValuesFrom rdf:resource="http://www.w3.org/2001/XMLSchema#decimal"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </rdfs:range>
        <rdfs:comment xml:lang="en">Relates a vessel to its dimension C, i. e. the distance from the GPS antenna to the port side.</rdfs:comment>
        <rdfs:label xml:lang="en">has dimension C</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://burbachs.github.io/AISHIPOntology/AISHIP#hasDimensionD -->


    <owl:ObjectProperty rdf:about="https://burbachs.github.io/AISHIPOntology/AISHIP#hasDimensionD">
        <rdfs:domain rdf:resource="http://www.vesselAI-project.eu/ontology#Vessel"/>
        <rdfs:range>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://qudt.org/schema/qudt/QuantityValue"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://qudt.org/schema/qudt/unit"/>
                        <owl:hasValue rdf:resource="https://qudt.org/3.1.2/vocab/unitM"/>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://qudt.org/schema/qudt/numericValue"/>
                        <owl:allValuesFrom rdf:resource="http://www.w3.org/2001/XMLSchema#decimal"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </rdfs:range>
        <rdfs:comment xml:lang="en">Relates a vessel to its dimension D, i. e. the distance from the GPS antenna to the starboard side.</rdfs:comment>
        <rdfs:label xml:lang="en">has dimension D</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://burbachs.github.io/AISHIPOntology/AISHIP#hasDraught -->


    <owl:ObjectProperty rdf:about="https://burbachs.github.io/AISHIPOntology/AISHIP#hasDraught">
        <rdfs:domain rdf:resource="http://www.vesselAI-project.eu/ontology#TrajectoryPart"/>
        <rdfs:range>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://qudt.org/schema/qudt/QuantityValue"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://qudt.org/schema/qudt/unit"/>
                        <owl:hasValue rdf:resource="https://qudt.org/3.1.2/vocab/unitM"/>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://qudt.org/schema/qudt/numericValue"/>
                        <owl:allValuesFrom rdf:resource="http://www.w3.org/2001/XMLSchema#decimal"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </rdfs:range>
        <rdfs:comment xml:lang="en">Relates a vessel to its draught, i.e., the vertical distance between the waterline and the lowest point of the hull (typically the keel), typically measured in meters (m).</rdfs:comment>
        <rdfs:label xml:lang="en">has draught</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://burbachs.github.io/AISHIPOntology/AISHIP#hasGrossTn -->


    <owl:ObjectProperty rdf:about="https://burbachs.github.io/AISHIPOntology/AISHIP#hasGrossTn">
        <rdfs:domain rdf:resource="http://www.vesselAI-project.eu/ontology#Vessel"/>
        <rdfs:range>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://qudt.org/schema/qudt/QuantityValue"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://qudt.org/schema/qudt/unit"/>
                        <owl:hasValue rdf:resource="https://qudt.org/3.1.2/vocab/unitGT"/>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://qudt.org/schema/qudt/numericValue"/>
                        <owl:allValuesFrom rdf:resource="http://www.w3.org/2001/XMLSchema#decimal"/>
                    </owl:Restriction>
                </owl:intersectionOf>
