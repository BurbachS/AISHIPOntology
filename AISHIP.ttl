@prefix : <http://www.semanticweb.org/simonburbach/AISHIP/> .
@prefix geo: <http://www.opengis.net/ont/geosparql#> .
@prefix nfo: <http://www.semanticdesktop.org/ontologies/2007/03/22/nfo#> .
@prefix nie: <http://www.semanticdesktop.org/ontologies/2007/01/19/nie#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix vai: <http://www.vesselAI-project.eu/ontology#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix qudt: <http://qudt.org/schema/qudt/> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix time: <http://www.w3.org/2006/time#> .
@prefix unit: <https://qudt.org/3.1.2/vocab/unit> .
@prefix nexif: <https://www.semanticdesktop.org/ontologies/2007/05/10/nexif/> .
@prefix schema: <https://schema.org/> .
@base <http://www.semanticweb.org/simonburbach/AISHIP/> .

<http://www.semanticweb.org/simonburbach/AISHIP> rdf:type owl:Ontology ;
                                                  owl:imports <http://qudt.org/3.1.2/schema/qudt> ,
                                                              rdf:Ontology .

#################################################################
#    Object Properties
#################################################################

###  http://www.semanticweb.org/simonburbach/AISHIP#drivenBy
:drivenBy rdf:type owl:ObjectProperty ;
          owl:inverseOf :drives ;
          rdfs:domain :MotorizedPropulsionSystem ;
          rdfs:range schema:EngineSpecification ;
          rdfs:comment "Relates a motorized propulsion technology with the engine it is driven by."@en .


###  http://www.semanticweb.org/simonburbach/AISHIP#drives
:drives rdf:type owl:ObjectProperty ;
        rdfs:domain schema:EngineSpecification ;
        rdfs:range :MotorizedPropulsionTechnology ;
        rdfs:comment "Relates an engine to the motorized propulsion technology it drives."@en .


###  http://www.semanticweb.org/simonburbach/AISHIP#hasAntiCavitationMechanism
:hasAntiCavitationMechanism rdf:type owl:ObjectProperty ;
                            owl:inverseOf :isAntiCavitationMechanismFor ;
                            rdfs:domain :MotorizedPropulsionTechnology ;
                            rdfs:range :AntiCavitationMechanism ;
                            rdfs:comment "Relates a motorized propulsion technology to its anti-cavitation mechanism that reduces its cavitation."@en .


###  http://www.semanticweb.org/simonburbach/AISHIP#hasDiameter
:hasDiameter rdf:type owl:ObjectProperty ;
             rdfs:domain :BladedPropulsor ;
             rdfs:range qudt:QuantityValue ;
             rdfs:comment "Relates a bladed propulsor to its diameter."@en .


###  http://www.semanticweb.org/simonburbach/AISHIP#isAntiCavitationMechanismFor
:isAntiCavitationMechanismFor rdf:type owl:ObjectProperty ;
                              rdfs:domain :AntiCavitationMechanism ;
                              rdfs:range :MotorizedPropulsionTechnology ;
                              rdfs:comment "Relates an anti-cavitation mechanism to the motorized propulsion technology it reduces the cavitation of."@en .


###  http://www.semanticweb.org/simonburbach/AISHIP#usedByPropulsionSystem
:usedByPropulsionSystem rdf:type owl:ObjectProperty ;
                        owl:inverseOf :usesPropulsionTechnology ;
                        rdfs:domain :PropulsionTechnology ;
                        rdfs:range :PropulsionSystem ;
                        rdfs:comment "Relates a propulsion technology to the propulsion system it is used by to create thrust."@en .


###  http://www.semanticweb.org/simonburbach/AISHIP#usesPropulsionTechnology
:usesPropulsionTechnology rdf:type owl:ObjectProperty ;
                          rdfs:domain :PropulsionSystem ;
                          rdfs:range :PropulsionTechnology ;
                          rdfs:comment "Relates a propulsion system to the propulsion technology it uses to create thrust."@en .


###  http://www.semanticweb.org/simonburbach/AISHIP/computedOverTimeInterval
:computedOverTimeInterval rdf:type owl:ObjectProperty ;
                          rdfs:domain :TypicalLocation ;
                          rdfs:range time:Interval ;
                          rdfs:comment "Relatesa typical location with the time interval it was computed over."@en .


###  http://www.semanticweb.org/simonburbach/AISHIP/hasAverageSpeed
:hasAverageSpeed rdf:type owl:ObjectProperty ;
                 rdfs:domain vai:Vessel ;
                 rdfs:range qudt:QuantityValue ;
                 rdfs:comment "Relates a vessel with its overall average speed, typically measured in knots (kn)."@en .


###  http://www.semanticweb.org/simonburbach/AISHIP/hasAverageTrajectorySpeed
:hasAverageTrajectorySpeed rdf:type owl:ObjectProperty ;
                           rdfs:domain vai:Trajectory ;
                           rdfs:range qudt:QuantityValue ;
                           rdfs:comment "Relates a trajtectory to the average speed of the vessel during that trip, typically measured in knots (kn)."@en .


###  http://www.semanticweb.org/simonburbach/AISHIP/hasDescription
:hasDescription rdf:type owl:ObjectProperty ;
                owl:inverseOf :isDescriptionOf ;
                rdfs:domain vai:Vessel ;
                rdfs:range nfo:TextDocument ;
                rdfs:comment "Relates a vessel to its text document description."@en .


###  http://www.semanticweb.org/simonburbach/AISHIP/hasDimA
:hasDimA rdf:type owl:ObjectProperty ;
         rdfs:domain vai:Vessel ;
         rdfs:range qudt:QuantityValue ;
         rdfs:comment "Relates a vessel to its dimension A, i. e. the distance from the GPS antenna to the bow."@en .


###  http://www.semanticweb.org/simonburbach/AISHIP/hasDimB
:hasDimB rdf:type owl:ObjectProperty ;
         rdfs:domain vai:Vessel ;
         rdfs:range qudt:QuantityValue ;
         rdfs:comment "Relates a vessel to its dimension B, i. e. the distance from the GPS antenna to the stern."@en .


###  http://www.semanticweb.org/simonburbach/AISHIP/hasDimC
:hasDimC rdf:type owl:ObjectProperty ;
         rdfs:domain vai:Vessel ;
         rdfs:range qudt:QuantityValue ;
         rdfs:comment "Relates a vessel to its dimension C, i. e. the distance from the GPS antenna to the port side."@en .


###  http://www.semanticweb.org/simonburbach/AISHIP/hasDimD
:hasDimD rdf:type owl:ObjectProperty ;
         rdfs:domain vai:Vessel ;
         rdfs:range qudt:QuantityValue ;
         rdfs:comment "Relates a vessel to its dimension D, i. e. the distance from the GPS antenna to the starboard side."@en .


###  http://www.semanticweb.org/simonburbach/AISHIP/hasDraught
:hasDraught rdf:type owl:ObjectProperty ;
            rdfs:domain vai:TrajectoryPart ;
            rdfs:range qudt:QuantityValue ;
            rdfs:comment "Relates a vessel to its draught, i.e., the vertical distance between the waterline and the lowest point of the hull (typically the keel), typically measured in meters (m)."@en .


###  http://www.semanticweb.org/simonburbach/AISHIP/hasOwner
:hasOwner rdf:type owl:ObjectProperty ;
          owl:inverseOf :ownsVessel ;
          rdfs:domain vai:Vessel ;
          rdfs:range :Owner ;
          rdfs:comment "Relates a vessel to its owner"@en .


###  http://www.semanticweb.org/simonburbach/AISHIP/hasPhoto
:hasPhoto rdf:type owl:ObjectProperty ;
          owl:inverseOf :isPhotoOf ;
          rdfs:domain vai:Vessel ;
          rdfs:range nexif:Photo ;
          rdfs:comment "Relates a vessel to its photo."@en .


###  http://www.semanticweb.org/simonburbach/AISHIP/hasSonarSignature
:hasSonarSignature rdf:type owl:ObjectProperty ;
                   owl:inverseOf :isSonarSignatureOf ;
                   rdfs:domain vai:Vessel ;
                   rdfs:range nfo:Audio ;
                   rdfs:comment "Relates a vessel to its sonar signature audio file."@en .


###  http://www.semanticweb.org/simonburbach/AISHIP/hasTypicalGeometry
:hasTypicalGeometry rdf:type owl:ObjectProperty ;
                    rdfs:subPropertyOf vai:hasGeometry ;
                    rdfs:domain :TypicalLocation ;
                    rdfs:range geo:Geometry ;
                    rdfs:comment "Relates a typical location of a vessel with its geometry."@en .


###  http://www.semanticweb.org/simonburbach/AISHIP/hasTypicalLocation
:hasTypicalLocation rdf:type owl:ObjectProperty ;
                    rdfs:domain vai:Vessel ;
                    rdfs:range :TypicalLocation ;
                    rdfs:comment "Related a vessel with its typical location."@en .


###  http://www.semanticweb.org/simonburbach/AISHIP/isDescriptionOf
:isDescriptionOf rdf:type owl:ObjectProperty ;
                 rdfs:domain nfo:TextDocument ;
                 rdfs:range vai:Vessel ;
                 rdfs:comment "Relates a text document description to a vessel"@en .


###  http://www.semanticweb.org/simonburbach/AISHIP/isPhotoOf
:isPhotoOf rdf:type owl:ObjectProperty ;
           rdfs:domain nexif:Photo ;
           rdfs:range vai:Vessel ;
           rdfs:comment "Relates a photo to its vessel."@en .


###  http://www.semanticweb.org/simonburbach/AISHIP/isSonarSignatureOf
:isSonarSignatureOf rdf:type owl:ObjectProperty ;
                    rdfs:domain nfo:Audio ;
                    rdfs:range vai:Vessel ;
                    rdfs:comment "Relates a sonar signature audio file to its vessel."@en .


###  http://www.semanticweb.org/simonburbach/AISHIP/ownsVessel
:ownsVessel rdf:type owl:ObjectProperty ;
            rdfs:domain :Owner ;
            rdfs:range vai:Vessel ;
            rdfs:comment "Relates an owner to its vessel."@en .


###  http://www.semanticweb.org/simonburbach/AISHIP/poweredBy
:poweredBy rdf:type owl:ObjectProperty ;
           owl:inverseOf :powers ;
           rdfs:domain schema:EngineSpecification ;
           rdfs:range :PowerGenerationUnit ;
           rdfs:comment "Relates an engine to the power generation unit it is powered by."@en .


###  http://www.semanticweb.org/simonburbach/AISHIP/powers
:powers rdf:type owl:ObjectProperty ;
        rdfs:domain :PowerGenerationUnit ;
        rdfs:range schema:EngineSpecification ;
        rdfs:comment "Relates a power generation unit to the engine it powers."@en .


###  http://www.semanticweb.org/simonburbach/AISHIP/propelledBy
:propelledBy rdf:type owl:ObjectProperty ;
             owl:inverseOf :propells ;
             rdfs:domain vai:Vessel ;
             rdfs:range :PropulsionSystem ;
             rdfs:comment "Relates a vessel to the propulsion system it is propelled by."@en .


###  http://www.semanticweb.org/simonburbach/AISHIP/propells
:propells rdf:type owl:ObjectProperty ;
          rdfs:domain :PropulsionSystem ;
          rdfs:range vai:Vessel ;
          rdfs:comment "Relates a propulsion system to the vessel it propells."@en .


#################################################################
#    Data properties
#################################################################

###  http://www.semanticweb.org/simonburbach/AISHIP#hasNumberOfBlades
:hasNumberOfBlades rdf:type owl:DatatypeProperty ;
                   rdfs:domain :BladedPropulsor ;
                   rdfs:range xsd:int ;
                   rdfs:comment "Relates a bladed propulsor to its number of blades."@en .


###  http://www.semanticweb.org/simonburbach/AISHIP/hasHIN
:hasHIN rdf:type owl:DatatypeProperty ;
        rdfs:domain vai:Vessel ;
        rdfs:range xsd:decimal ;
        rdfs:comment "Relates a vessel with its HIN."@en .


#################################################################
#    Classes
#################################################################

###  http://www.semanticdesktop.org/ontologies/2007/03/22/nfo#Audio
nfo:Audio rdf:type owl:Class ;
          rdfs:comment "A text document."@en .


###  http://www.semanticdesktop.org/ontologies/2007/03/22/nfo#TextDocument
nfo:TextDocument rdf:type owl:Class ;
                 rdfs:comment "A file containing audio content."@en .


###  http://www.semanticweb.org/simonburbach/AISHIP#AntiCavitationMechanism
:AntiCavitationMechanism rdf:type owl:Class ;
                         rdfs:comment "A design feature or technology applied to propulsion systems to prevent or reduce cavitation effects on propeller blades or related components. Such mechanisms aim to minimize efficiency loss, noise, and material damage caused by cavitation."@en .


###  http://www.semanticweb.org/simonburbach/AISHIP#BladedPropulsor
:BladedPropulsor rdf:type owl:Class ;
                 rdfs:subClassOf :MotorizedPropulsionTechnology ;
                 rdfs:comment "A motorized propulsion technology that generates thrust using rotating blades to accelerate water. This category includes conventional propellers and specialized blade-based systems designed for various operational profiles."@en .


###  http://www.semanticweb.org/simonburbach/AISHIP#ControllablePitchPropeller
:ControllablePitchPropeller rdf:type owl:Class ;
                            rdfs:subClassOf :ScrewPropeller ;
                            rdfs:comment "A screw propeller with blades that can adjust their pitch during operation, allowing flexible control of thrust and speed without changing the rotation direction. Frequently used in ships that require frequent speed or direction changes."@en .


###  http://www.semanticweb.org/simonburbach/AISHIP#CounterRotatingPropeller
:CounterRotatingPropeller rdf:type owl:Class ;
                          rdfs:subClassOf :ScrewPropeller ;
                          rdfs:comment "A screw propeller system that uses two coaxial propellers rotating in opposite directions to improve efficiency and reduce rotational losses. This design enhances thrust and is typically used on high-performance vessels."@en .


###  http://www.semanticweb.org/simonburbach/AISHIP#DuctedPropeller
:DuctedPropeller rdf:type owl:Class ;
                 rdfs:subClassOf :ScrewPropeller ;
                 rdfs:comment "A screw propeller enclosed in a fixed nozzle (duct) to increase thrust and efficiency at lower speeds and provide additional protection. Widely used in tugs and fishing trawlers requiring high bollard pull."@en .


###  http://www.semanticweb.org/simonburbach/AISHIP#FixedPitchPropeller
:FixedPitchPropeller rdf:type owl:Class ;
                     rdfs:subClassOf :ScrewPropeller ;
                     rdfs:comment "A screw propeller with blades fixed at a constant angle. It offers simple and cost-effective design, commonly used in vessels with steady operating conditions where speed and load vary little."@en .


###  http://www.semanticweb.org/simonburbach/AISHIP#FlettnerRotor
:FlettnerRotor rdf:type owl:Class ;
               rdfs:subClassOf :RotorSails ;
               rdfs:comment "A specific type of rotor sail consisting of a tall, rotating cylinder mounted vertically on the deck. When wind flows across the spinning surface, lift is generated via the Magnus effect, assisting vessel propulsion."@en .


###  http://www.semanticweb.org/simonburbach/AISHIP#GasWaterJet
:GasWaterJet rdf:type owl:Class ;
             rdfs:subClassOf :JetPropulsor ;
             rdfs:comment "A jet propulsion technology that uses gas (e.g., exhaust gases from a turbine) instead of a pumpt to accelerate water and create thrust. This system combines combustion energy with fluid dynamics for high-performance propulsion."@en .


###  http://www.semanticweb.org/simonburbach/AISHIP#HullForm
:HullForm rdf:type owl:Class ;
          rdfs:subClassOf :WindAssistedPropulsionTechnology ;
          rdfs:comment "A wind-assisted propulsion technology that utilizes the vesselâ€™s hull geometry to generate additional aerodynamic lift or reduce drag, complementing primary propulsion systems."@en .


###  http://www.semanticweb.org/simonburbach/AISHIP#JetPropulsor
:JetPropulsor rdf:type owl:Class ;
              rdfs:subClassOf :MotorizedPropulsionTechnology ;
              rdfs:comment "A motorized propulsion technology that generates thrust by accelerating a fluid (typically water) through a nozzle, creating a jet stream that propels the vessel. Jet propulsors are commonly used for high-speed boats and specialized vessels."@en .


###  http://www.semanticweb.org/simonburbach/AISHIP#Kite
:Kite rdf:type owl:Class ;
      rdfs:subClassOf :WindAssistedPropulsionTechnology ;
      rdfs:comment "A wind-assisted propulsion technology that uses a large controllable kite deployed ahead of the vessel to harness wind energy and reduce engine power requirements."@en .


###  http://www.semanticweb.org/simonburbach/AISHIP#MotorizedPropulsionTechnology
:MotorizedPropulsionTechnology rdf:type owl:Class ;
                               rdfs:subClassOf :PropulsionTechnology ;
                               rdfs:comment "A propulsion technology that relies on mechanical motors or engine to convert energy into thrust for vessel propulsion."@en .


###  http://www.semanticweb.org/simonburbach/AISHIP#PaddleWheel
:PaddleWheel rdf:type owl:Class ;
             rdfs:subClassOf :BladedPropulsor ;
             rdfs:comment "A propulsion technology that uses large external wheels with attached blades or paddles to push water and generate thrust. Historically used in riverboats and still applied in some shallow-water or heritage vessels."@en .


###  http://www.semanticweb.org/simonburbach/AISHIP#PropellerPod
:PropellerPod rdf:type owl:Class ;
              rdfs:subClassOf :SpeciallyDesignedBladedPropulsor ;
              rdfs:comment "A propulsion system where the propeller is mounted on a steerable pod that also contains the drive motor. Podded propulsors allow full 360-degree rotation for excellent maneuverability and are commonly used in cruise ships and offshore vessels."@en .


###  http://www.semanticweb.org/simonburbach/AISHIP#PropulsionTechnology
:PropulsionTechnology rdf:type owl:Class ;
                      rdfs:comment "A general concept representing the method or technology used by a propulsion system to generate thrust for vessel movement. This class provides an abstraction for different approaches such as motor-driven or wind-based techniques."@en .


###  http://www.semanticweb.org/simonburbach/AISHIP#PureWindPropulsionTechnology
:PureWindPropulsionTechnology rdf:type owl:Class ;
                              rdfs:subClassOf :WindPropulsionTechnology ;
                              rdfs:comment "A wind propulsion technology that exclusively uses wind as the energy source for generating thrust, without assistance from motorized systems. Typical implementation involves sails or other aerodynamic devices."@en .


###  http://www.semanticweb.org/simonburbach/AISHIP#RotorSails
:RotorSails rdf:type owl:Class ;
            rdfs:subClassOf :WindAssistedPropulsionTechnology ;
            rdfs:comment "A wind-assisted propulsion technology that uses rotating cylindrical sails (rotors) to create aerodynamic lift through the Magnus effect, reducing engine load and fuel consumption."@en .


###  http://www.semanticweb.org/simonburbach/AISHIP#RudderPropeller
:RudderPropeller rdf:type owl:Class ;
                 rdfs:subClassOf :ScrewPropeller ;
                 rdfs:comment "A screw propeller integrated with a steerable drive unit, enabling both propulsion and steering without a separate rudder. Common in azimuth thrusters for offshore vessels and ships needing high maneuverability."@en .


###  http://www.semanticweb.org/simonburbach/AISHIP#ScrewPropeller
:ScrewPropeller rdf:type owl:Class ;
                rdfs:subClassOf :BladedPropulsor ;
                rdfs:comment "A widely used bladed propulsion technology where helical blades mounted on a rotating hub create thrust by pushing water backwards. Screw propellers are commonly used for most vessel types due to their simplicity and efficiency."@en .


###  http://www.semanticweb.org/simonburbach/AISHIP#SpeciallyDesignedBladedPropulsor
:SpeciallyDesignedBladedPropulsor rdf:type owl:Class ;
                                  rdfs:subClassOf :BladedPropulsor ;
                                  rdfs:comment "A bladed propulsion technology adapted for specific vessel requirements or operational conditions, such as high maneuverability or integrated drive systems."@en .


###  http://www.semanticweb.org/simonburbach/AISHIP#SuctionWings
:SuctionWings rdf:type owl:Class ;
              rdfs:subClassOf :WindAssistedPropulsionTechnology ;
              rdfs:comment "A wind-assisted propulsion technology using rigid wing structures with suction or airflow control systems to enhance aerodynamic lift and reduce fuel consumption."@en .


###  http://www.semanticweb.org/simonburbach/AISHIP#SupercavitatingPropeller
:SupercavitatingPropeller rdf:type owl:Class ;
                          rdfs:subClassOf :ScrewPropeller ;
                          rdfs:comment "A screw propeller designed to operate under conditions of intentional cavitation, reducing drag at very high speeds. Common in fast military or racing vessels where conventional propellers would suffer performance loss."@en .


###  http://www.semanticweb.org/simonburbach/AISHIP#VoithSchneiderPropeller
:VoithSchneiderPropeller rdf:type owl:Class ;
                         rdfs:subClassOf :SpeciallyDesignedBladedPropulsor ;
                         rdfs:comment "A highly maneuverable bladed propulsion system using vertical blades that rotate and adjust their pitch cyclically. This design enables precise control in all directions, making it ideal for tugs, ferries, and vessels operating in confined waters."@en .


###  http://www.semanticweb.org/simonburbach/AISHIP#WaterJet
:WaterJet rdf:type owl:Class ;
          rdfs:subClassOf :JetPropulsor ;
          rdfs:comment "A jet propulsion technology that draws water into an intake and expels it at high velocity through a nozzle to generate thrust. Water jets are widely used for high-speed ferries, jet boats, and shallow-water applications."@en .


###  http://www.semanticweb.org/simonburbach/AISHIP#WindAssistedPropulsionTechnology
:WindAssistedPropulsionTechnology rdf:type owl:Class ;
                                  rdfs:subClassOf :WindPropulsionTechnology ;
                                  rdfs:comment "A hybrid propulsion technology where wind energy supplements motorized propulsion to reduce fuel consumption and improve efficiency. Examples include rotor sails, suction wings, or kites."@en .


###  http://www.semanticweb.org/simonburbach/AISHIP#WindPropulsionTechnology
:WindPropulsionTechnology rdf:type owl:Class ;
                          rdfs:subClassOf :PropulsionTechnology ;
                          rdfs:comment "A propulsion technology that harnesses wind energy, typically through aerodynamic surfaces or devices, to provide thrust for moving a vessel."@en .


###  http://www.semanticweb.org/simonburbach/AISHIP#WindTurbine
:WindTurbine rdf:type owl:Class ;
             rdfs:subClassOf :WindAssistedPropulsionTechnology ;
             rdfs:comment "A wind-assisted propulsion technology using rotor blades similar to wind power generators, converting wind energy into thrust or auxiliary electrical energy for propulsion."@en .


###  http://www.semanticweb.org/simonburbach/AISHIP/BatteryUnit
:BatteryUnit rdf:type owl:Class ;
             rdfs:subClassOf :PowerGenerationUnit ;
             rdfs:comment "An energy storage system that supplies electrical power for propulsion or auxiliary systems, charged from onboard or external sources."@en .


###  http://www.semanticweb.org/simonburbach/AISHIP/CODAD
:CODAD rdf:type owl:Class ;
       rdfs:subClassOf :CombinedEngine ;
       rdfs:comment "A combined propulsion system that uses multiple diesel engines in parallel to provide flexible power management and redundancy."@en .


###  http://www.semanticweb.org/simonburbach/AISHIP/CODAG
:CODAG rdf:type owl:Class ;
       rdfs:subClassOf :CombinedEngine ;
       rdfs:comment "A combined propulsion system that can use both a diesel engine and a gas turbine simultaneously for increased power output."@en .


###  http://www.semanticweb.org/simonburbach/AISHIP/CODLAG
:CODLAG rdf:type owl:Class ;
        rdfs:subClassOf :CombinedEngine ;
        rdfs:comment "A combined propulsion system that integrates diesel-electric drive for cruising and a gas turbine for greater speed when needed."@en .


###  http://www.semanticweb.org/simonburbach/AISHIP/CODOG
:CODOG rdf:type owl:Class ;
       rdfs:subClassOf :CombinedEngine ;
       rdfs:comment "A combined propulsion system that uses either a diesel engine for efficient cruising or a gas turbine for high-speed operation."@en .


###  http://www.semanticweb.org/simonburbach/AISHIP/COGAG
:COGAG rdf:type owl:Class ;
       rdfs:subClassOf :CombinedEngine ;
       rdfs:comment "A combined propulsion system using multiple gas turbines, with smaller units for efficient cruising and additional turbines engaged for high-speed operation."@en .


###  http://www.semanticweb.org/simonburbach/AISHIP/COGAS
:COGAS rdf:type owl:Class ;
       rdfs:subClassOf :CombinedEngine ;
       rdfs:comment "A combined propulsion system that integrates a gas turbine and a steam turbine, often in a combined-cycle configuration for efficiency."@en .


###  http://www.semanticweb.org/simonburbach/AISHIP/COGES
:COGES rdf:type owl:Class ;
       rdfs:subClassOf :CombinedEngine ;
       rdfs:comment "A combined propulsion system using gas and steam turbines to generate electricity, which powers electric motors for propulsion."@en .


###  http://www.semanticweb.org/simonburbach/AISHIP/COGOG
:COGOG rdf:type owl:Class ;
       rdfs:subClassOf :CombinedEngine ;
       rdfs:comment "A combined propulsion system with two gas turbines, where only one operates at a time depending on the desired speed profile."@en .


###  http://www.semanticweb.org/simonburbach/AISHIP/CONAG
:CONAG rdf:type owl:Class ;
       rdfs:subClassOf :CombinedEngine ;
       rdfs:comment "An experimental propulsion system combining a nuclear reactor with a gas turbine, typically used in advanced naval vessels."@en .


###  http://www.semanticweb.org/simonburbach/AISHIP/CONAN
:CONAN rdf:type owl:Class ;
       rdfs:subClassOf :CombinedEngine ;
       rdfs:comment "An experimental propulsion system using two nuclear reactors for increased redundancy and sustained power output."@en .


###  http://www.semanticweb.org/simonburbach/AISHIP/CONAS
:CONAS rdf:type owl:Class ;
       rdfs:subClassOf :CombinedEngine ;
       rdfs:comment "A combined propulsion system that uses a nuclear reactor to generate steam, which drives a steam turbine for propulsion."@en .


###  http://www.semanticweb.org/simonburbach/AISHIP/COSAG
:COSAG rdf:type owl:Class ;
       rdfs:subClassOf :CombinedEngine ;
       rdfs:comment "A combined propulsion system using a steam turbine for cruising and a gas turbine for rapid acceleration, with optional simultaneous operation."@en .


###  http://www.semanticweb.org/simonburbach/AISHIP/CombinedEngine
:CombinedEngine rdf:type owl:Class ;
                rdfs:subClassOf schema:EngineSpecification ;
                rdfs:comment "A propulsion system that integrates two or more engine types to optimize performance, efficiency, or redundancy."@en .


###  http://www.semanticweb.org/simonburbach/AISHIP/DieselCombustionUnit
:DieselCombustionUnit rdf:type owl:Class ;
                      rdfs:subClassOf :PowerGenerationUnit ;
                      rdfs:comment "A power unit that generates mechanical or electrical energy by combusting diesel fuel. Often used to drive generators or directly coupled engines."@en .


###  http://www.semanticweb.org/simonburbach/AISHIP/DieselEngine
:DieselEngine rdf:type owl:Class ;
              rdfs:subClassOf :PistonEngine ;
              rdfs:comment "An internal combustion engine that uses the compression ignition of diesel fuel to produce mechanical motion."@en .


