#################################################################
#    Annotation properties
#################################################################

###  http://purl.org/dc/terms/bibliographicCitation
dct:bibliographicCitation rdf:type owl:AnnotationProperty .


###  http://purl.org/ontology/bibo/doi
bibo:doi rdf:type owl:AnnotationProperty .


###  http://xmlns.com/foaf/0.1/homepage
foaf:homepage rdf:type owl:AnnotationProperty .


###  http://xmlns.com/foaf/0.1/mbox
foaf:mbox rdf:type owl:AnnotationProperty .


###  http://xmlns.com/foaf/0.1/name
foaf:name rdf:type owl:AnnotationProperty .


#################################################################
#    Object Properties
#################################################################

###  http://qudt.org/schema/qudt/unit
qudt:unit rdf:type owl:ObjectProperty .


###  https://github.com/BurbachS/AISHIPOntology#computedOverTimeInterval
:computedOverTimeInterval rdf:type owl:ObjectProperty ;
                          rdfs:domain :TypicalLocation ;
                          rdfs:range time:Interval ;
                          rdfs:comment "Relatesa typical location with the time interval it was computed over."@en ;
                          rdfs:label "computed over time interval"@en .


###  https://github.com/BurbachS/AISHIPOntology#drivenBy
:drivenBy rdf:type owl:ObjectProperty ;
          owl:inverseOf :drives ;
          rdfs:domain :MotorizedPropulsionSystem ;
          rdfs:range schema:EngineSpecification ;
          rdfs:comment "Relates a motorized propulsion technology with the engine it is driven by."@en ;
          rdfs:label "driven by"@en .


###  https://github.com/BurbachS/AISHIPOntology#drives
:drives rdf:type owl:ObjectProperty ;
        rdfs:domain schema:EngineSpecification ;
        rdfs:range :MotorizedPropulsionTechnology ;
        rdfs:comment "Relates an engine to the motorized propulsion technology it drives."@en ;
        rdfs:label "drives"@en .


###  https://github.com/BurbachS/AISHIPOntology#hasActualHeight
:hasActualHeight rdf:type owl:ObjectProperty ;
                 rdfs:domain vai:Vessel ;
                 rdfs:range [ owl:intersectionOf ( qudt:QuantityValue
                                                   [ rdf:type owl:Restriction ;
                                                     owl:onProperty qudt:unit ;
                                                     owl:hasValue unit:M
                                                   ]
                                                   [ rdf:type owl:Restriction ;
                                                     owl:onProperty qudt:numericValue ;
                                                     owl:allValuesFrom xsd:decimal
                                                   ]
                                                 ) ;
                              rdf:type owl:Class
                            ] ;
                 rdfs:comment "Relates a vessel to its actual height. Indicates the actual current height of the vessel, e.g., considering load or structural changes, expressed in meters. Note: This property is semantically the same as http://www.vesselAI-project.eu/ontology#hasActualHeight. However, we allow the expression as a quantity with numeric value and a unit."@en ;
                 rdfs:label "has actual height"@en .


###  https://github.com/BurbachS/AISHIPOntology#hasAntiCavitationMechanism
:hasAntiCavitationMechanism rdf:type owl:ObjectProperty ;
                            owl:inverseOf :isAntiCavitationMechanismFor ;
                            rdfs:domain :MotorizedPropulsionTechnology ;
                            rdfs:range :AntiCavitationMechanism ;
                            rdfs:comment "Relates a motorized propulsion technology to its anti-cavitation mechanism that reduces its cavitation."@en ;
                            rdfs:label "has anti-cavitation mechanism"@en .


###  https://github.com/BurbachS/AISHIPOntology#hasAverageSpeedOverGround
:hasAverageSpeedOverGround rdf:type owl:ObjectProperty ;
                           rdfs:domain vai:Vessel ;
                           rdfs:range [ owl:intersectionOf ( qudt:QuantityValue
                                                             [ rdf:type owl:Restriction ;
                                                               owl:onProperty qudt:unit ;
                                                               owl:hasValue unit:KN
                                                             ]
                                                             [ rdf:type owl:Restriction ;
                                                               owl:onProperty qudt:numericValue ;
                                                               owl:allValuesFrom xsd:decimal
                                                             ]
                                                           ) ;
                                        rdf:type owl:Class
                                      ] ;
                           rdfs:comment "Relates a vessel with its overall average speed over ground, typically measured in knots (kn)."@en ;
                           rdfs:label "has average speed over ground"@en .


###  https://github.com/BurbachS/AISHIPOntology#hasAverageTrajectorySpeedOverGround
:hasAverageTrajectorySpeedOverGround rdf:type owl:ObjectProperty ;
                                     rdfs:domain vai:Trajectory ;
                                     rdfs:range [ owl:intersectionOf ( qudt:QuantityValue
                                                                       [ rdf:type owl:Restriction ;
                                                                         owl:onProperty qudt:unit ;
                                                                         owl:hasValue unit:KN
                                                                       ]
                                                                       [ rdf:type owl:Restriction ;
                                                                         owl:onProperty qudt:numericValue ;
                                                                         owl:allValuesFrom xsd:decimal
                                                                       ]
                                                                     ) ;
                                                  rdf:type owl:Class
                                                ] ;
                                     rdfs:comment "Relates a trajtectory to the average speed over ground of the vessel during that trip, typically measured in knots (kn)."@en ;
                                     rdfs:label "has average trajectory speed over ground"@en .


###  https://github.com/BurbachS/AISHIPOntology#hasCourseOverGround
:hasCourseOverGround rdf:type owl:ObjectProperty ;
                     rdfs:domain vai:Vessel ;
                     rdfs:range [ owl:intersectionOf ( qudt:QuantityValue
                                                       [ rdf:type owl:Restriction ;
                                                         owl:onProperty qudt:unit ;
                                                         owl:hasValue unit:DEG
                                                       ]
                                                       [ rdf:type owl:Restriction ;
                                                         owl:onProperty qudt:numericValue ;
                                                         owl:allValuesFrom [ rdf:type rdfs:Datatype ;
                                                                             owl:onDatatype xsd:decimal ;
                                                                             owl:withRestrictions ( [ xsd:minInclusive 0.0
                                                                                                    ]
                                                                                                    [ xsd:maxInclusive 360.0
                                                                                                    ]
                                                                                                  )
                                                                           ]
                                                       ]
                                                     ) ;
                                  rdf:type owl:Class
                                ] ;
                     rdfs:comment "Relates a vessel to its course over ground. Indicates the actual course of a vessel over the ground, given as an angle relative to true north (in degrees). Note: This property is semantically the same as http://www.vesselAI-project.eu/ontology#hasCourseOverGround. However, we allow the expression as a quantity with numeric value and a unit."@en ;
                     rdfs:label "has course over ground"@en .


###  https://github.com/BurbachS/AISHIPOntology#hasDeadweight
:hasDeadweight rdf:type owl:ObjectProperty ;
               rdfs:domain vai:Vessel ;
               rdfs:range [ owl:intersectionOf ( qudt:QuantityValue
                                                 [ rdf:type owl:Restriction ;
                                                   owl:onProperty qudt:unit ;
                                                   owl:hasValue unit:TON_Metric
                                                 ]
                                                 [ rdf:type owl:Restriction ;
                                                   owl:onProperty qudt:numericValue ;
                                                   owl:allValuesFrom xsd:decimal
                                                 ]
                                               ) ;
                            rdf:type owl:Class
                          ] ;
               rdfs:comment "Relates a vessel to its deadweight. Deadweight tonnage (DWT) represents the maximum weight of cargo, fuel, crew, provisions, and other supplies the vessel can carry, expressed in metric tons. Note: This property is semantically the same as http://www.vesselAI-project.eu/ontology#hasDeadWeight. However, we allow the expression as a quantity with numeric value and a unit."@en ;
               rdfs:label "has deadweight"@en .


###  https://github.com/BurbachS/AISHIPOntology#hasDescription
:hasDescription rdf:type owl:ObjectProperty ;
                owl:inverseOf :isDescriptionOf ;
                rdfs:domain vai:Vessel ;
                rdfs:range nfo:TextDocument ;
                rdfs:comment "Relates a vessel to its text document description."@en ;
                rdfs:label "has description"@en .


###  https://github.com/BurbachS/AISHIPOntology#hasDiameter
:hasDiameter rdf:type owl:ObjectProperty ;
             rdfs:domain :BladedPropulsor ;
             rdfs:range [ owl:intersectionOf ( qudt:QuantityValue
                                               [ rdf:type owl:Restriction ;
                                                 owl:onProperty qudt:unit ;
                                                 owl:hasValue unit:CentiM
                                               ]
                                               [ rdf:type owl:Restriction ;
                                                 owl:onProperty qudt:numericValue ;
                                                 owl:allValuesFrom xsd:decimal
                                               ]
                                             ) ;
                          rdf:type owl:Class
                        ] ;
             rdfs:comment "Relates a bladed propulsor to its diameter."@en ;
             rdfs:label "has diameter"@en .


###  https://github.com/BurbachS/AISHIPOntology#hasDimensionA
:hasDimensionA rdf:type owl:ObjectProperty ;
               rdfs:domain vai:Vessel ;
               rdfs:range [ owl:intersectionOf ( qudt:QuantityValue
                                                 [ rdf:type owl:Restriction ;
                                                   owl:onProperty qudt:unit ;
                                                   owl:hasValue unit:M
                                                 ]
                                                 [ rdf:type owl:Restriction ;
                                                   owl:onProperty qudt:numericValue ;
                                                   owl:allValuesFrom xsd:decimal
                                                 ]
                                               ) ;
                            rdf:type owl:Class
                          ] ;
               rdfs:comment "Relates a vessel to its dimension A, i. e. the distance from the GPS antenna to the bow."@en ;
               rdfs:label "has dimension A"@en .


###  https://github.com/BurbachS/AISHIPOntology#hasDimensionB
:hasDimensionB rdf:type owl:ObjectProperty ;
               rdfs:domain vai:Vessel ;
               rdfs:range [ owl:intersectionOf ( qudt:QuantityValue
                                                 [ rdf:type owl:Restriction ;
                                                   owl:onProperty qudt:unit ;
                                                   owl:hasValue unit:M
                                                 ]
                                                 [ rdf:type owl:Restriction ;
                                                   owl:onProperty qudt:numericValue ;
                                                   owl:allValuesFrom xsd:decimal
                                                 ]
                                               ) ;
                            rdf:type owl:Class
                          ] ;
               rdfs:comment "Relates a vessel to its dimension B, i. e. the distance from the GPS antenna to the stern."@en ;
               rdfs:label "has dimension B"@en .


###  https://github.com/BurbachS/AISHIPOntology#hasDimensionC
:hasDimensionC rdf:type owl:ObjectProperty ;
               rdfs:domain vai:Vessel ;
               rdfs:range [ owl:intersectionOf ( qudt:QuantityValue
                                                 [ rdf:type owl:Restriction ;
                                                   owl:onProperty qudt:unit ;
                                                   owl:hasValue unit:M
                                                 ]
                                                 [ rdf:type owl:Restriction ;
                                                   owl:onProperty qudt:numericValue ;
                                                   owl:allValuesFrom xsd:decimal
                                                 ]
                                               ) ;
                            rdf:type owl:Class
                          ] ;
               rdfs:comment "Relates a vessel to its dimension C, i. e. the distance from the GPS antenna to the port side."@en ;
               rdfs:label "has dimension C"@en .


###  https://github.com/BurbachS/AISHIPOntology#hasDimensionD
:hasDimensionD rdf:type owl:ObjectProperty ;
               rdfs:domain vai:Vessel ;
               rdfs:range [ owl:intersectionOf ( qudt:QuantityValue
                                                 [ rdf:type owl:Restriction ;
                                                   owl:onProperty qudt:unit ;
                                                   owl:hasValue unit:M
                                                 ]
                                                 [ rdf:type owl:Restriction ;
                                                   owl:onProperty qudt:numericValue ;
                                                   owl:allValuesFrom xsd:decimal
                                                 ]
                                               ) ;
                            rdf:type owl:Class
                          ] ;
               rdfs:comment "Relates a vessel to its dimension D, i. e. the distance from the GPS antenna to the starboard side."@en ;
               rdfs:label "has dimension D"@en .


###  https://github.com/BurbachS/AISHIPOntology#hasDraught
:hasDraught rdf:type owl:ObjectProperty ;
            rdfs:domain vai:TrajectoryPart ;
            rdfs:range [ owl:intersectionOf ( qudt:QuantityValue
                                              [ rdf:type owl:Restriction ;
                                                owl:onProperty qudt:unit ;
                                                owl:hasValue unit:M
                                              ]
                                              [ rdf:type owl:Restriction ;
                                                owl:onProperty qudt:numericValue ;
                                                owl:allValuesFrom xsd:decimal
                                              ]
                                            ) ;
                         rdf:type owl:Class
                       ] ;
            rdfs:comment "Relates a vessel to its draught, i.e., the vertical distance between the waterline and the lowest point of the hull (typically the keel), typically measured in meters (m)."@en ;
            rdfs:label "has draught"@en .


###  https://github.com/BurbachS/AISHIPOntology#hasGrossTn
:hasGrossTn rdf:type owl:ObjectProperty ;
            rdfs:domain vai:Vessel ;
            rdfs:range [ owl:intersectionOf ( qudt:QuantityValue
                                              [ rdf:type owl:Restriction ;
                                                owl:onProperty qudt:unit ;
                                                owl:hasValue unit:GT
                                              ]
                                              [ rdf:type owl:Restriction ;
                                                owl:onProperty qudt:numericValue ;
                                                owl:allValuesFrom xsd:decimal
                                              ]
                                            ) ;
                         rdf:type owl:Class
                       ] ;
            rdfs:comment "Relates a vessel to its gross tonnage (GT). Indicates the gross tonnage (GT) of the vessel, a dimensionless index defined by the IMO Tonnage Convention based on the ship's enclosed volume. Note: This property is semantically the same as http://www.vesselAI-project.eu/ontology#hasGrossTn. However, we allow the expression as a quantity with numeric value and a unit."@en ;
            rdfs:label "has gross tn"@en .


###  https://github.com/BurbachS/AISHIPOntology#hasHeading
:hasHeading rdf:type owl:ObjectProperty ;
            rdfs:domain vai:Vessel ;
            rdfs:range [ owl:intersectionOf ( qudt:QuantityValue
                                              [ rdf:type owl:Restriction ;
                                                owl:onProperty qudt:unit ;
                                                owl:hasValue unit:DEG
                                              ]
                                              [ rdf:type owl:Restriction ;
                                                owl:onProperty qudt:numericValue ;
                                                owl:allValuesFrom [ rdf:type rdfs:Datatype ;
                                                                    owl:onDatatype xsd:decimal ;
                                                                    owl:withRestrictions ( [ xsd:minInclusive 0.0
                                                                                           ]
                                                                                           [ xsd:maxInclusive 360.0
                                                                                           ]
                                                                                         )
                                                                  ]
                                              ]
                                            ) ;
                         rdf:type owl:Class
                       ] ;
            rdfs:comment "Relates a vessel to its heading. Indicates the orientation of the vessel's bow relative to true or magnetic north, expressed as an angle in degrees. Note: This property is semantically the same as http://www.vesselAI-project.eu/ontology#hasHeading. However, we allow the expression as a quantity with numeric value and a unit."@en ;
            rdfs:label "has heading"@en .


###  https://github.com/BurbachS/AISHIPOntology#hasHeight
:hasHeight rdf:type owl:ObjectProperty ;
           rdfs:domain vai:Vessel ;
           rdfs:range [ owl:intersectionOf ( qudt:QuantityValue
                                             [ rdf:type owl:Restriction ;
                                               owl:onProperty qudt:unit ;
                                               owl:hasValue unit:M
                                             ]
                                             [ rdf:type owl:Restriction ;
                                               owl:onProperty qudt:numericValue ;
                                               owl:allValuesFrom xsd:decimal
                                             ]
                                           ) ;
                        rdf:type owl:Class
                      ] ;
           rdfs:comment "Relates a vessel to its height. Indicates the overall height of the vessel, generally from the keel to the highest point, expressed in meters. Note: This property is semantically the same as http://www.vesselAI-project.eu/ontology#hasHeight. However, we allow the expression as a quantity with numeric value and a unit."@en ;
           rdfs:label "has height"@en .


###  https://github.com/BurbachS/AISHIPOntology#hasLength
:hasLength rdf:type owl:ObjectProperty ;
           rdfs:domain vai:Vessel ;
           rdfs:range [ owl:intersectionOf ( qudt:QuantityValue
                                             [ rdf:type owl:Restriction ;
                                               owl:onProperty qudt:unit ;
                                               owl:hasValue unit:M
                                             ]
                                             [ rdf:type owl:Restriction ;
                                               owl:onProperty qudt:numericValue ;
                                               owl:allValuesFrom xsd:decimal
                                             ]
                                           ) ;
                        rdf:type owl:Class
                      ] ;
           rdfs:comment "Relates a vessel to its length. Indicates the overall length of the vessel, typically measured in meters. Note: This property is semantically the same as http://www.vesselAI-project.eu/ontology#hasLength. However, we allow the expression as a quantity with numeric value and a unit."@en ;
           rdfs:label "has length"@en .


###  https://github.com/BurbachS/AISHIPOntology#hasOwner
:hasOwner rdf:type owl:ObjectProperty ;
          owl:inverseOf :isOwnerOf ;
          rdfs:domain vai:Vessel ;
          rdfs:range :Owner ;
          rdfs:comment "Relates a vessel to its owner"@en ;
          rdfs:label "has owner"@en .


###  https://github.com/BurbachS/AISHIPOntology#hasPhoto
:hasPhoto rdf:type owl:ObjectProperty ;
          owl:inverseOf :isPhotoOf ;
          rdfs:domain vai:Vessel ;
          rdfs:range nexif:Photo ;
          rdfs:comment "Relates a vessel to its photo."@en ;
          rdfs:label "has photo"@en .


###  https://github.com/BurbachS/AISHIPOntology#hasSonarSignature
:hasSonarSignature rdf:type owl:ObjectProperty ;
                   owl:inverseOf :isSonarSignatureOf ;
                   rdfs:domain vai:Vessel ;
                   rdfs:range nfo:Audio ;
                   rdfs:comment "Relates a vessel to its sonar signature audio file."@en ;
                   rdfs:label "has sonar signature"@en .


###  https://github.com/BurbachS/AISHIPOntology#hasSpeedOverGround
:hasSpeedOverGround rdf:type owl:ObjectProperty ;
                    rdfs:domain vai:Vessel ;
                    rdfs:range [ owl:intersectionOf ( qudt:QuantityValue
                                                      [ rdf:type owl:Restriction ;
                                                        owl:onProperty qudt:unit ;
                                                        owl:hasValue unit:KN
                                                      ]
                                                      [ rdf:type owl:Restriction ;
                                                        owl:onProperty qudt:numericValue ;
                                                        owl:allValuesFrom xsd:decimal
                                                      ]
                                                    ) ;
                                 rdf:type owl:Class
                               ] ;
                    rdfs:comment "Relates a vessel to its speed over ground (SOG). SOG indicates the speed of a vessel relative to the ground surface. Not to be confused with speed through water. Note: This property is semantically the same as http://www.vesselAI-project.eu/ontology#hasSpeedOverGround. However, we allow the expression as a quantity with numeric value and a unit."@en ;
                    rdfs:label "has speed over ground"@en .


###  https://github.com/BurbachS/AISHIPOntology#hasTypicalGeometry
:hasTypicalGeometry rdf:type owl:ObjectProperty ;
                    rdfs:subPropertyOf vai:hasGeometry ;
                    rdfs:domain :TypicalLocation ;
                    rdfs:range geo:Geometry ;
                    rdfs:comment "Relates a typical location of a vessel with its geometry."@en ;
                    rdfs:label "has typical geometry"@en .


###  https://github.com/BurbachS/AISHIPOntology#hasTypicalLocation
:hasTypicalLocation rdf:type owl:ObjectProperty ;
                    rdfs:domain vai:Vessel ;
                    rdfs:range :TypicalLocation ;
                    rdfs:comment "Related a vessel with its typical location."@en ;
                    rdfs:label "has typical location"@en .


###  https://github.com/BurbachS/AISHIPOntology#hasWidth
:hasWidth rdf:type owl:ObjectProperty ;
          rdfs:domain vai:Vessel ;
          rdfs:range [ owl:intersectionOf ( qudt:QuantityValue
                                            [ rdf:type owl:Restriction ;
                                              owl:onProperty qudt:unit ;
                                              owl:hasValue unit:M
                                            ]
                                            [ rdf:type owl:Restriction ;
                                              owl:onProperty qudt:numericValue ;
                                              owl:allValuesFrom xsd:decimal
                                            ]
                                          ) ;
                       rdf:type owl:Class
                     ] ;
          rdfs:comment "Relates a vessel to its width. Indicates the overall width (beam) of the vessel, measured in meters. Note: This property is semantically the same as http://www.vesselAI-project.eu/ontology#hasWidth. However, we allow the expression as a quantity with numeric value and a unit."@en ;
          rdfs:label "has width"@en .


###  https://github.com/BurbachS/AISHIPOntology#isAntiCavitationMechanismFor
:isAntiCavitationMechanismFor rdf:type owl:ObjectProperty ;
                              rdfs:domain :AntiCavitationMechanism ;
                              rdfs:range :MotorizedPropulsionTechnology ;
                              rdfs:comment "Relates an anti-cavitation mechanism to the motorized propulsion technology it reduces the cavitation of."@en ;
                              rdfs:label "is anti-cavitation mechanism for"@en .


